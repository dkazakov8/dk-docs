{
  "name": "dk-docs",
  "version": "1.0.0",
  "description": "",
  "author": "Dmitry Kazakov",
  "license": "MIT",
  "scripts": {
    "prepare": "husky install",
    "first-run": "pnpm i&&cp example.dev.env .env&&cd ./tests&&pnpm i&& cd ../",
    "upd": "rm -rf ./node_modules&&pnpm i --prefer-offline",
    "upd-prod": "rm -rf ./node_modules&&pnpm i --prefer-offline --prod --ignore-scripts",

    "build": "node -r @swc-node/register -r dotenv/config ./_esbuild/builderEsbuild.ts",
    "start": "node -r dotenv/config ./build/server.js",

    "analyze:js": "eslint --ignore-path .formatignore --ext \".js,.ts,.tsx\" ./",
    "analyze:style": "stylelint --ignore-path .formatignore \"**/*.scss\"",

    "format:js": "eslint --ignore-path .formatignore --ext \".js,.ts,.tsx\" --fix",
    "format:style": "stylelint --ignore-path .formatignore --fix",

    "check-types": "tsc",
    "check-types-precommit": "tsc --project tsconfig-precommit.json"
  },
  "browserslist": [
    "Safari >= 13",
    "last 5 Edge versions",
    "last 5 Opera versions",
    "last 5 Chrome versions",
    "last 5 Firefox versions"
  ],
  "dependencies": {
    "@mantine/core": "7.2.2",
    "@mantine/hooks": "7.2.2",
    "@mantine/code-highlight": "7.2.2",
    "@loadable/component": "5.15.3",
    "antd": "5.11.5",
    "axios": "1.4.0",
    "classnames": "2.3.2",
    "dk-bff-server": "3.1.37",
    "dk-localize": "3.1.37",
    "dk-mobx-restore-state": "3.1.37",
    "dk-react-mobx-globals": "3.1.37",
    "dk-react-mobx-router": "3.1.37",
    "dk-react-mobx-config-form": "3.1.37",
    "dotenv": "16.3.1",
    "history": "5.3.0",
    "lodash": "4.17.21",
    "mobx": "6.12.0",
    "mobx-react": "8.0.0",
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "esbuild-plugin-browserslist": "0.10.0",
    "browserslist": "4.22.1",
    "@craftamap/esbuild-plugin-html": "0.6.1",
    "@swc-node/register": "1.6.8",
    "@swc/core": "1.3.99",
    "@types/classnames": "2.3.0",
    "@types/express": "4.17.17",
    "@types/fs-extra": "11.0.1",
    "@types/loadable__component": "5.13.4",
    "@types/lodash": "4.14.195",
    "@types/node": "20.3.1",
    "@types/react": "18.2.13",
    "@types/react-dom": "18.2.6",
    "@types/ws": "8.5.5",
    "better-spawn": "1.0.4",
    "chalk": "4.1.2",
    "css-loader": "6.8.1",
    "dk-esbuild-plugin-replace": "0.0.10",
    "esbuild-plugin-inject-preload": "0.0.6",
    "esbuild-plugin-webpack-analyzer": "0.0.5",
    "dk-compare-env": "3.1.37",
    "dk-eslint-config": "3.1.17",
    "dk-file-generator": "3.1.37",
    "dk-reload-server": "3.1.37",
    "esbuild": "0.19.8",
    "esbuild-plugin-compress": "1.0.1",
    "esbuild-sass-plugin": "2.16.0",
    "fs-extra": "11.1.1",
    "husky": "8.0.3",
    "lint-staged": "13.0.3",
    "node-dev": "7.4.3",
    "postcss-modules": "6.0.0",
    "typescript": "5.0.4"
  },
